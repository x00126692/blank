# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page

GET     /                           	controllers.HomeController.index
GET	  /blank			controllers.HomeController.blank
GET	  /buttons			controllers.HomeController.buttons
GET	   /flot			controllers.HomeController.flot
GET	   /forms			controllers.HomeController.forms
GET 	/grid			controllers.HomeController.grid
GET	  /icons			controllers.HomeController.icons
GET 	/morris			controllers.HomeController.morris
GET 	/notifications				controllers.HomeController.notifications
GET 	/panels-wells			controllers.HomeController.panels-wells
GET 	/tables			controllers.HomeController.tables
GET 	/typography			controllers.HomeController.typography
		

# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
